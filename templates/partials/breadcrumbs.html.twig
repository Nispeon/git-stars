<section id="breadcrumbs" class="breadcrumbs">
    <div class="container">
        <div class="d-flex justify-content-between align-items-center">
            <h2></h2>
            <ol>
                <li><a href="{{ path('app_index') }}">Home</a></li>
                {% for item in links %}
                    {% if item.link is not null %}
                        <li><a href="{{ item.link }}">{{ item.title }}</a></li>
                    {% else %}
                        <li>{{ item.title }}</li>
                    {% endif %}
                {% endfor %}
            </ol>
        </div>
    </div>
</section>